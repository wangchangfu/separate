<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapscience.modular.system.mapper.SourceEntryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mapscience.modular.system.model.SourceEntry">
        <id column="source_entry_id" property="sourceEntryId" />
        <result column="source_entry_name" property="sourceEntryName" />
        <result column="status" property="status" />
        <result column="crate_time" property="crateTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        source_entry_id, source_entry_name, status, crate_time, update_time
    </sql>
    <select id="getSourceEntryByName" resultMap="BaseResultMap">
        select * from t_source_entry where source_entry_name = #{sourceEntryName}
    </select>

</mapper>
